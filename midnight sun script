local function killauraforautofarm()
    spawn(function()
if getgenv().autofarm == true then
while getgenv().autofarm do task.wait()
    pcall( function()
    local args = {
        [1] = "M1"
    }
    
    game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool").RemoteEvent:FireServer(unpack(args))
    task.wait()
    local args = {
        [1] = "Hitbox",
        [2] = 1,
        [3] = "M1"
    }
    
    game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool").RemoteEvent:FireServer(unpack(args))
    end)
end
else
    return
end
end)
end

local function autofarm(demon)
spawn(function()
if getgenv().autofarm == true then

    while getgenv().autofarm do task.wait()

    if game:GetService("Workspace").Live.Npcs:FindFirstChild(demon) then
       
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Live.Npcs[demon].HumanoidRootPart.CFrame * CFrame.new(0,position,0)

    end

    end
else
    return
end
end)
end



local function killaura()
    spawn(function()
if getgenv().killaura == true then
while getgenv().killaura do task.wait()
    pcall( function()
    local args = {
        [1] = "M1"
    }
    
    game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool").RemoteEvent:FireServer(unpack(args))
    task.wait()
    local args = {
        [1] = "Hitbox",
        [2] = 1,
        [3] = "M1"
    }
    
    game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool").RemoteEvent:FireServer(unpack(args))
    end)
end
else
    return
end
end)
end


local function godmode()
spawn (function()
if getgenv().godmode == true then
    
    while getgenv().godmode do task.wait()
    game.Players.LocalPlayer.Character.Dash.RemoteEvent:FireServer("Falldmg", 0/0);
    
    end
else

return

end
end)
end

local function autobreathing()
    spawn(function()
if getgenv().autobreathing == true then
    
    local args = {
        [1] = "Start"
    }
    
    game:GetService("Players").LocalPlayer.Character.Breath.RemoteEvent:FireServer(unpack(args))

else 
return
end
end)
end




local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Midnight sun v2.00 by Mac#0014 and HTDBarsi <3", "DarkTheme")

local Tab = Window:NewTab("Autofarm")


local Section = Tab:NewSection("Autofarms(use noclip to avoid glitches)")

Section:NewToggle("Autofarm", "Toggles Autofarm", function(state)
    if state then
        getgenv().autofarm = true 
        
        
killauraforautofarm()
autofarm(demon)

    else
        getgenv().autofarm = false
        killauraforautofarm()
autofarm(demon)

    end
end)

Section:NewDropdown("Choose Mob", "Chooses mob for the autofarm to farm", {"Weak", "Strong", "Ultra", "Mizunoto","Kanato","Tsuchinoe", "Hinoe",}, function(currentOption)
    demon = currentOption

end)
Section:NewToggle("Auto grip(for slayer npcs)", "Automaticaly grips slayer npcs", function(state)
    if state then
        getgenv().autogrip = true 
        
        
autogrip()
    else
        getgenv().autogrip = false
        autogrip()
    end
end)

Section:NewSlider("Farm position()", "Farm position", 7, -6, function(s) -- 500 (MaxValue) | 0 (MinValue)
    position = s
end)





local Section = Tab:NewSection("Autofarm Bosses(use noclip and godmode to avoid glitches)")


Section:NewDropdown("Choose boss to farm(must be spawned)", "Boss to choose", {"Akaza", "Shura"}, function(currentOption)
    bosses = currentOption
end)

Section:NewButton("Boss spawner", "Spawns the boss for you", function()
if bosses == "Akaza" then
akazaspawner()

elseif bosses == "Shura" then
shuraspawner()

else
    return
end
    end)

    


Section:NewToggle("Autofarm Bosses(use god mode, best position is 3)", "Toggles Bosses Autofarm", function(state)
    if state then
        getgenv().bossautofarm = true 
       killauraforboss() 
boss()

    else
        getgenv().bossautofarm = false
        killauraforboss()
boss()

    end
end)


Section:NewSlider("Farm position for ", "Farm position", 7, -7, function(s) -- 500 (MaxValue) | 0 (MinValue)
    position1 = s
end)
local Tab = Window:NewTab("Event Npc autotp")
local Section = Tab:NewSection("Event Npc autotp")
Section:NewToggle("Auto Event Npc teleporter", "Automatically teleports you to an npc if it spawned.", function(state)
    if state then
        getgenv().findnpc = true
        findnpc1()


    else
        getgenv().findnpc = false
       findnpc1()

    end
end)
Section:NewDropdown("Choose Event Npc to look for", "Choose an Npc", {"Muzan", "Merchant"}, function(currentOption)
    npclookingfor = currentOption
end)









